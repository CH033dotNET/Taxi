@model Model.User

@{
    ViewBag.Title = "ChangeMenu";
}
@Html.ValidationSummary()
@using (Html.BeginForm("ChangeMenu"))
    {
<table>
    <tr>
        <td>@Html.LabelFor(x => Model.Id)</td>
        <td>@Html.LabelFor(x => Model.UserName)</td>
        <td>@Html.LabelFor(x => Model.Password)</td>
        <td>@Html.LabelFor(x => Model.Email)</td>
        <td>@Html.LabelFor(x => Model.Role.Name)</td>
    </tr>
       @Html.Partial("_EditUser",Model)
</table>
      <input type="submit" name="Ok" value="Ok" />
    } 
